/**app.wxss**/
page {
    width: 100%;
    height: 100%;
    font-size: 20rpx;
    color: #333;
    background-color: #ededed;
    box-sizing: border-box;
}
.flex-row {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
}
.flex-column {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  padding: 40rpx 0;
  box-sizing: border-box;
} 
.section{
  width: 92%;
  margin: auto;
}

